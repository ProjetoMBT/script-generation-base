<UserControl x:Class="UmlComponentDiagramEditor.Notation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="150" MinHeight="100" MinWidth="150" Width="Auto" Height="Auto"
             HorizontalAlignment="Left" VerticalAlignment="Top" Background="Transparent" MouseLeftButtonDown="UserControl_MouseLeftButtonDown">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/UmlComponentDiagramEditor;component/Images/NotationWhite.png" />
        </Grid.Background>
        <TextBlock Name="TextBlock_Text" FontSize="9" Margin="5"/>
        <Label Content="minSelection= " Name="lblMinSel" MouseDoubleClick="lblMinSel_MouseDoubleClick" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Label Content="maxSelection= " Name="lblMaxSel" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,17,0,0" MouseDoubleClick="lblMaxSel_MouseDoubleClick"/>
        <Label Content="Variants=" Name="lblVariants" Margin="0,34,0,0"/>
    </Grid>
</UserControl>
